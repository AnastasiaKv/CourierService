    <script>
        function lableStatus() {
            var type = document.getElementById("status_lable").valueOf();

            if (type == 'Completed')
                document.getElementById("status_lable").className = "label label-primary";
            if (type == 'Shipped')
                document.getElementById("status_lable").className = "label label-success";
            if (type == 'Processing')
                document.getElementById("status_lable").className = "label label-info";
            if (type == 'Damaged')
                document.getElementById("status_lable").className = "label label-warning";
            if (type == 'Canceled')
                document.getElementById("status_lable").className = "label label-danger";
        }
    </script>
