@using Resources
@{
    ViewBag.Title = Resources.OrderHistory;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@Resources.OrderHistory</title>
    <!-- Bootstrap -->
    <link href="/Content/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="/Content/assets/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/Content/assets/css/bootstrap.vertical-tabs.css">
    <link rel="stylesheet" href="/Content/packages/perfect-scrollbar/perfect-scrollbar.css">
    <link rel="stylesheet" href="/Content/packages/touchspin/jquery.bootstrap-touchspin.css">
    <link rel="stylesheet" href="/Content/packages/jasny/css/jasny-bootstrap.min.css">
    <link href="/Content/assets/css/styles.css" rel="stylesheet">
</head>

<body id="page-history" class="page-history">

    <div class="container">

        <div class="row">

            <!-- start sidebar navigation -->
            <div class="col-md-2">
                <!-- start my account panel -->
                <div class="panel panel-sidebar">
                    <div class="panel-heading">@Resources.MyAccount</div>
                    <div class="panel-body">
                        <ul class="nav nav-list">
                            <li>
                                <a href="@Url.Action("Index", "UserProfile")"><i class=" fa fa-user"></i> @Resources.EditProfile</a>
                            </li>
                            <li class="active">
                                <a href="@Url.Action("Index", "OrderHistory")"><i class=" fa fa-history"></i>@Resources.OrderHistoryUp</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <!-- end my account panel -->
            </div>
            <!-- end sidebar navigation -->
            <!-- start history main content -->
            <div class="col-md-10">
                <!-- start page title -->
                <h1 class="page-title">@Resources.OrderHistory</h1>
                <!-- end page title -->
                <!-- start history table -->
                <table class="table table-striped tcart">
                    <!-- start table header -->
                    <thead>
                        <tr>
                            <th>@Resources.Date</th>
                            <th>ID</th>
                            <th>@Resources.Price</th>
                            <th>@Resources.Status</th>
                        </tr>
                    </thead>
                    <!-- end table header -->
                    <tbody>
                        <tr>
                            <td>30.05.2015</td>
                            <td>1111d22c-9704-e511-8295-28e3477cfb43</td>
                            <td>120.00</td>
                            <td>
                                <span id="status_label" value="" class="label label-success">Shipped</span>
                            </td>
                        </tr>
                        <tr>
                            <td>29.05.2015</td>
                            <td>c433d22c-9704-e511-8295-28e3477cfb43</td>
                            <td>143.00</td>
                            <td>
                                <span id="status_label" value="" class="label label-info">Processing</span>
                            </td>
                        </tr>
                        <tr>
                            <td>05.06.2015</td>
                            <td>c233222c-9704-e511-824f5-28e3477cfb43</td>
                            <td>215.00</td>
                            <td>
                                <span id="status_label" value="" class="label label-danger">Cancled</span>
                            </td>
                        </tr>
                        <tr>
                            <td>11.06.2015</td>
                            <td>c231d22c-9094-e511-8295-28e3477cfb43</td>
                            <td>80.00</td>
                            <td>
                                <span id="status_label" value="" class="label label-warning">Damaged</span>
                            </td>
                        </tr>
                        <tr>
                            <td>30.05.2015</td>
                            <td>c231d22c-9704-e511-8295-28e323kcfb43</td>
                            <td>240.00</td>
                            <td>
                                <span id="status_label" value="" class="label label-primary">Complated</span>
                            </td>
                        </tr>
                        <tr>
                            <td>26.05.2015</td>
                            <td>c231d22c-9704-e511-8295-1023477cfb43</td>
                            <td>280.00</td>
                            <td>
                                <span id="status_label" value="" class="label label-success">Shipped</span>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <!-- end history table -->

            </div>
            <!-- end history main content -->

        </div>

    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="/Content/assets/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/Content/packages/jssor/js/jssor.js"></script>
    <script type="text/javascript" src="/Content/packages/jssor/js/jssor.slider.js"></script>
    <script type="text/javascript" src="/Content/packages/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script type="text/javascript" src="/Content/packages/iCheck/icheck.js"></script>
    <script type="text/javascript" src="/Content/packages/multirowselect/multirowselect.js"></script>
    <script type="text/javascript" src="/Content/packages/ion.rangeslider/js/ion.rangeSlider.js"></script>
    <script type="text/javascript" src="/Content/packages/elevatezoom/jquery.elevatezoom.js"></script>
    <script type="text/javascript" src="/Content/packages/touchspin/jquery.bootstrap-touchspin.js"></script>
    <script type="text/javascript" src="/Content/packages/jasny/js/jasny-bootstrap.js"></script>
    <script type="text/javascript" src="/Content/assets/js/app.js"></script>
</body>

</html>


